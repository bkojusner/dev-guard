{
    "name": "extension-security-auditor",
    "displayName": "Extension Security Auditor for Cursor",
    "description": "A simple security auditor for Cursor extensions that checks for known malicious extensions.",
    "version": "0.0.1",
    "publisher": "your-publisher-name",
    "author": "your-name",
    "engines": {
      "vscode": "^1.80.0"
    },
    "categories": [
      "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
      "commands": [
        {
          "command": "extension-security-auditor.checkExtensions",
          "title": "Extension Security Auditor: Check Installed Extensions"
        }
      ]
    },
    "devDependencies": {
      "@types/vscode": "^1.80.0",
      "typescript": "^5.0.0"
    }
  }